        <!-- Splash screens. One is randomly chosen when FlightGear starts -->
        <startup>
            <splash-texture>Aircraft/c150/splash1.png</splash-texture>
        </startup>

        <!-- Fix scrolling direction of knobs -->
        <mouse>
            <invert-mouse-wheel type="bool">true</invert-mouse-wheel>
