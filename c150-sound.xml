    <breaker>
        <name>breaker</name>
        <mode>once</mode>
        <path>Sounds/click3.wav</path>
        <condition>
            <property>/sim/model/c150/sound/breakers</property>
        </condition>
        <position>
            <x>0.5</x>
            <y>0.0</y>
            <z>0.0</z>
        </position>
        <reference-dist>0.2</reference-dist>
        <max-dist>5.0</max-dist>
    </breaker>

 </fx>
 
</PropertyList>
